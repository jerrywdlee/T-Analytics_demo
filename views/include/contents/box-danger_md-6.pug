// USERS LIST
.box.box-danger
  .box-header.with-border
    h3.box-title Latest Members
    .box-tools.pull-right
      span.label.label-danger 8 New Members
      button.btn.btn-box-tool(type='button', data-widget='collapse')
        i.fa.fa-minus
      button.btn.btn-box-tool(type='button', data-widget='remove')
        i.fa.fa-times
  // /.box-header
  .box-body.no-padding
    ul.users-list.clearfix
      li
        img(src='dist/img/user1-128x128.jpg', alt='User Image')
        a.users-list-name(href='#') Alexander Pierce
